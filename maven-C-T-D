    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'Hello World'
            }
        }
        stage ('compile')
{ 
     steps
{
echo 'hi there '
}
}
     stage ('build')
{ 
     steps
{
echo 'hi there '
}
}
stage('Test') {
           
 steps {
                echo 'Hello Jenkins'
            }
        }
stage ('deploy')
{ 
     steps
{
echo 'hi devops '
}
   }

    }
}
